tion_count":23,"metadata":{"executionInfo":{"elapsed":3,"status":"ok","timestamp":1709721222338,"user":{"displayName":"Amaljith U K","userId":"12965048598593176892"},"user_tz":0},"id":"ROBQQLmNwGbb"},"outputs":[],"source":["from torch.utils.data import DataLoader\n","\n","# Define the dataloaders\n","train_loader = DataLoader(train_dataset, batch_size=16, shuffle=True)\n","valid_loader = DataLoader(valid_dataset, batch_size=16)"]},{"cell_type":"code","execution_count":23,"metadata":{"executionInfo":{"elapsed":2,"status":"ok","timestamp":1709721222338,"user":{"displayName":"Amaljith U K","userId":"12965048598593176892"},"user_tz":0},"id":"-agTqwhaqtci"},"outputs":[],"source":[]},{"cell_type":"code","execution_count":24,"metadata":{"executionInfo":{"elapsed":3,"status":"ok","timestamp":1709721222339,"user":{"displayName":"Amaljith U K","userId":"12965048598593176892"},"user_tz":0},"id":"K8lPIP0zl5uD"},"outputs":[],"source":["#from transformers import BertForQuestionAnswering\n","\n","#model = BertForQuestionAnswering.from_pretrained(\"bert-base-uncased\")"]},{"cell_type":"markdown","metadata":{"id":"ybwjqNH72qAS"},"source":["### **Model definition ü§ñ**\n","\n","We are going to use the `bert-case-uncased` from the huggingface transformers."]},{"cell_type":"code","execution_count":25,"metadata":{"executionInfo":{"elapsed":6743,"status":"ok","timestamp":1709721229079,"user":{"displayName":"Amaljith U K","userId":"12965048598593176892"},"user_tz":0},"id":"bvOgUhOWeLIl","colab":{"base_uri":"https://localhost:8080/","height":101,"referenced_widgets":["d614d8d385d74584862a97eb0d81c20b","eadc7559a6f1420aaf94801bad5b1390","0cdb497597b64a78853ccbce97fde11b","b209201b088c49159ba332f6ead1d895","8d60769e0f994834a23813e23af0bc8f","39d4d8898c4f4432af845a28ba29fc33","f83c2b7d7cf14594bfea72b5a44d3ef0","d9137fe37bb6409098f4f4c9dc940732","6d28ae489386440986277ac6eb7516e9","8bbbacd44f044656a59a75e560e379ed","2a2ede39fd354fe38ffd0a24a809ab4e"]},"outputId":"4b0b6957-d0a4-42f7-8e53-859b8ecc37c1"},"outputs":[{"output_type":"display_data","data":{"text/plain":["model.safetensors:   0%|          | 0.00/440M [00:00<?, ?B/s]"],"application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"d614d8d385d74584862a97eb0d81c20b"}},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Outputs with dense layers: (tensor(1.6087, grad_fn=<DivBackward0>), tensor([[-0.0155, -0.0180, -0.0174, -0.0178, -0.0174]],\n","       grad_fn=<SqueezeBackward1>), tensor([[-0.0211, -0.0214, -0.0202, -0.0191, -0.0184]],\n","       grad_fn=<SqueezeBackward1>))\n"]}],"source":["import torch\n","import torch.nn as nn\n","import torch.nn.functional as F\n","from transformers import BertModel\n","\n","class BertMultiHopForQAWithDense(nn.Module):\n","    def __init__(self, num_hops=2, num_dense_layers=2, dense_hidden_size=768):\n","        super(BertMultiHopForQAWithDense, self).__init__()\n","\n","        # Load the pretrained BERT model of 12-layers with an uncased vocab.\n","        self.bert = BertModel.from_pretrained(\"bert-base-uncased\")\n","        self.hidden_size = self.bert.config.hidden_size\n","        self.num_hops = num_hops\n","\n","        # Additional layers for multi-hop reasoning\n","        self.additional_layers = nn.ModuleList([nn.Linear(self.hidden_size, self.hidden_size) for _ in range(num_hops)])\n","\n","        # Dense layers\n","        self.dense_layers = nn.ModuleList([nn.Linear(self.hidden_size, dense_hidden_size) for _ in range(num_dense_layers)])\n","\n","        # Task-specific output layers - one for the start position and the second for the end position of the answer.\n","        self.linear_start = nn.Linear(dense_hidden_size, 1)\n","        self.linear_end = nn.Linear(dense_hidden_size, 1)\n","\n","        # Freezing BERT's embeddings.\n","        for param in self.bert.embeddings.parameters():\n","            param.requires_grad = False\n","\n","        # Freezing the first 8 layers of BERT.\n","        for i in range(1, 9):\n","            for param in self.bert.encoder.layer[i].parameters():\n","                param.requires_grad = False\n","\n","        # Freezing the dense layers\n","        for dense_layer in self.dense_layers:\n","            for param in dense_layer.parameters():\n","                param.requires_grad = False\n","\n","    def forward(self, input_ids, attention_mask, token_type_ids, start_positions=None, end_positions=None):\n","        bert_output = self.bert(input_ids=input_ids, attention_mask=attention_mask, token_type_ids=token_type_ids)\n","        hidden_states = bert_output[0]\n","\n","        # Additional layers for multi-hop reasoning\n","        for layer in self.additional_layers:\n","            hidden_states = F.relu(layer(hidden_states))\n","\n","        # Additional dense layers\n","        for dense_layer in self.dense_layers:\n","            hidden_states = F.relu(dense_layer(hidden_states))\n","\n","        # Pass the hidden states through the linear layers to get the start and end logits.\n","        start_logits = self.linear_start(hidden_states).squeeze(-1)\n","        end_logits = self.linear_end(hidden_states).squeeze(-1)\n","\n","        outputs = (start_logits, end_logits)\n","\n","        if start_positions is not None and end_positions is not None:\n","            # Calculate the loss if start and end positions are provided\n","            loss_fct = nn.CrossEntropyLoss()\n","            start_loss = loss_fct(start_logits, start_positions)\n","            end_loss = loss_fct(end_logits, end_positions)\n","            loss = (start_loss + end_loss) / 2.0\n","            outputs = (loss,) + outputs\n","\n","        return outputs\n","\n","    def save_model(self, path):\n","        # Save the entire model or specific components\n","        torch.save(self.state_dict(), path)\n","    def load_model(self, path):\n","        self.load_state_dict(torch.load(path))\n","\n","\n","# Example usage\n","model= BertMultiHopForQAWithDense()\n","\n","# Sample input\n","input_ids = torch.tensor([[1, 2, 3, 4, 5]])# Each 'data' dict has two keys (title and paragraphs)\n","\n","attention_mask = torch.tensor([[1, 1, 1, 1, 1]])\n","token_type_ids = torch.tensor([[0, 0, 0, 0, 0]])\n","start_positions = torch.tensor([2])  # Replace with actual start position\n","end_positions = torch.tensor([4])  # Replace with actual end position\n","\n","# Forward pass with additional start and end positions\n","outputs_with_dense_layers = model(input_ids, attention_mask, token_type_ids, start_positions, end_positions)\n","\n","print(\"Outputs with dense layers:\", outputs_with_dense_layers)\n"]},{"cell_type":"code","execution_count":26,"metadata":{"executionInfo":{"elapsed":7,"status":"ok","timestamp":1709721229080,"user":{"displayName":"Amaljith U K","userId":"12965048598593176892"},"user_tz":0},"id":"Kh_2--xXwW6o","colab":{"base_uri":"https://localhost:8080/"},"outputId":"d4a0ef8e-8936-4ca4-d7e0-a81245204a55"},"outputs":[{"output_type":"stream","name":"stdout","text":["Working on cuda\n"]}],"source":["# Check on the available device - use GPU\n","device = torch.device('cuda') if torch.cuda.is_available() else torch.device('cpu')\n","print(f'Working on {device}')"]},{"cell_type":"code","execution_count":27,"metadata":{"executionInfo":{"elapsed":5,"status":"ok","timestamp":1709721229080,"user":{"displayName":"Amaljith U K","userId":"12965048598593176892"},"user_tz":0},"id":"57fJ86TuG2B-"},"outputs":[],"source":["#CUDA_LAUNCH_BLOCKING=1"]},{"cell_type":"code","execution_count":28,"metadata":{"executionInfo":{"elapsed":4,"status":"ok","timestamp":1709721229080,"user":{"displayName":"Amaljith U K","userId":"12965048598593176892"},"user_tz":0},"id":"j0fwWBDuVdR6"},"outputs":[],"source":["#import os\n","\n","#os.environ['CUDA_LAUNCH_BLOCKING'] = '1'\n"]},{"cell_type":"code","execution_count":29,"metadata":{"executionInfo":{"elapsed":7619,"status":"ok","timestamp":1709721236695,"user":{"displayName":"Amaljith U K","userId